    "three",
    "glslify",
    "gsap",
    "boilerplate",
    "webpack"
  ],
  "author": "Piotr Podziemski <piotr@holobits.pl> (http://holobits.pl)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/podski-holobits/threejs-webpack-kit/issues"
  },
  "homepage": "https://github.com/podski-holobits/threejs-webpack-kit#readme",
  "devDependencies": {
